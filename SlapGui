-- Players.chris_95910.PlayerGui.SlapGui.Frame.LoopSlapAll.LocalScript
local function C_5()
	local script = LMG2L["LocalScript_5"];
	_G.Toggle = false	
	
	local button = script.Parent	
	
	button.MouseButton1Click:Connect(function()	
	    _G.Toggle = not _G.Toggle	
	end)	
	
	while task.wait() do	
	    if _G.Toggle then	
	        for i, v in pairs(game.Players:GetPlayers()) do	
	            if v ~= game.Players.LocalPlayer and v.Character and v.Character:FindFirstChild("HumanoidRootPart") then	
	                local args = {	
	                    "slash",	
	                    v.Character,	
	                    Vector3.new(	
	                        v.Character.HumanoidRootPart.Position.X,	
	                        -10000,	
	                        v.Character.HumanoidRootPart.Position.Z	
	                    )	
	                }	
	
	                game:GetService("Players").LocalPlayer	
	                    :WaitForChild("Backpack")	
	                    :WaitForChild("Gold Slap")	
	                    :WaitForChild("Event")	
	                    :FireServer(unpack(args))	
	            end	
	        end	
	    end	
	end	
end;
task.spawn(C_5);
-- Players.chris_95910.PlayerGui.SlapGui.Frame.LocalScript
local function C_9()
	local script = LMG2L["LocalScript_9"];
	local frame = script.Parent	
	local UIS = game:GetService("UserInputService")	
	
	local dragging = false	
	local dragStart	
	local startPos	
	
	frame.InputBegan:Connect(function(input)	
		if input.UserInputType == Enum.UserInputType.Touch then	
			dragging = true	
			dragStart = input.Position	
			startPos = frame.Position	
		end	
	end)	
	
	frame.InputEnded:Connect(function(input)	
		if input.UserInputType == Enum.UserInputType.Touch then	
			dragging = false	
		end	
	end)	
	
	UIS.InputChanged:Connect(function(input)	
		if dragging and input.UserInputType == Enum.UserInputType.Touch then	
			local delta = input.Position - dragStart	
			frame.Position = UDim2.new(	
				startPos.X.Scale,	
				startPos.X.Offset + delta.X,	
				startPos.Y.Scale,	
				startPos.Y.Offset + delta.Y	
			)	
		end	
	end)	
end;
task.spawn(C_9);
-- Players.chris_95910.PlayerGui.SlapGui.Frame.UnLoopSlapAll.LocalScript
local function C_c()
	local script = LMG2L["LocalScript_c"];
	local button = script.Parent	
	
	button.MouseButton1Click:Connect(function()	
	    _G.Toggle = false	
	end)	
end;
task.spawn(C_c);
-- Players.chris_95910.PlayerGui.SlapGui.Frame.SlapButton.LocalScript
local function C_11()
	local script = LMG2L["LocalScript_11"];
	local button = script.Parent	
	local Players = game:GetService("Players")	
	local player = Players.LocalPlayer	
	local TextBox = button.Parent:WaitForChild("TextBox")	
	
	button.MouseButton1Click:Connect(function()	
		local target = Players:FindFirstChild(TextBox.Text)	
		if target and target.Character then	
			player.Backpack["Gold Slap"].Event:FireServer(	
				"slash",	
				target.Character,	
				Vector3.new(-5.462399005889893, 0, 49.70072555541992)	
			)	
		end	
	end)	
end;
task.spawn(C_11);
-- Players.chris_95910.PlayerGui.SlapGui.Frame.TextBox.LocalScript
local function C_15()
	local script = LMG2L["LocalScript_15"];
	local TextBox = script.Parent	
	local Players = game:GetService("Players")	
	
	TextBox.FocusLost:Connect(function(enterPressed)	
		if not enterPressed then return end	
		local input = TextBox.Text	
		if #input < 2 then return end -- minimum 2 lettres avant complÃ©tion	
	
		local lowerInput = input:lower()	
		for _, plr in pairs(Players:GetPlayers()) do	
			if plr.Name:lower():sub(1, #lowerInput) == lowerInput then	
				TextBox.Text = plr.Name	
				TextBox.CursorPosition = #plr.Name + 1	
				break	
			end	
		end	
	end)	
end;
task.spawn(C_15);

return LMG2L["SlapGui_1"], require;
